<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="INTEGER" hibernate-type="int"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="DOUBLE" hibernate-type="java.lang.Double"
			precision="22" not-null="false">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			length="45" not-null="false">
		</sql-type>
	</type-mapping>
	<table-filter match-name="items" match-catalog="ooad"></table-filter>
	<table-filter></table-filter>
	<table catalog="ooad" name="deposits"
		class="com.bid.data.Deposits">
		<column name="moneyFrozen" jdbc-type=''></column>
	</table>
	<table catalog="ooad" name="sorts" class="com.bid.data.Sorts">
		<column name="sortName" jdbc-type=''></column>
	</table>
	<table catalog="ooad" name="items" class="com.bid.data.Items">
		<column name="itemName" jdbc-type=''></column>
		<column name="itemDes" jdbc-type=''></column>
		<column name="itemBidRule" jdbc-type=''></column>
		<column name="itemFlourPrice" jdbc-type=''></column>
		<column name="ItemHighestBidprice" jdbc-type=''></column>
		<column name="itemHighestBidUserName" jdbc-type=''></column>
		<column name="itmeStatus" jdbc-type=''></column>
		<column name="itemAvailableSeconds" jdbc-type=''></column>
		<column name="itemCargoName" jdbc-type=''></column>
		<column name="itmeCargoID" jdbc-type=''></column>
		<column name="sortID" jdbc-type=''></column>
		<column name="postUser" jdbc-type=''></column>
	</table>
	<table catalog="ooad" name="users" class="com.bid.data.Users">
		<column name="userBalance" jdbc-type=''></column>
		<column name="userPass" jdbc-type=''></column>
		<column name="userBankAccount" jdbc-type=''></column>
		<column name="userMailBox" jdbc-type=''></column>
	</table>
</hibernate-reverse-engineering>